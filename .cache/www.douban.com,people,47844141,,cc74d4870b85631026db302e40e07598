    <h2>
        【FddddddY.】的豆瓣说&nbsp;&middot;&nbsp;&middot;&nbsp;&middot;&nbsp;&middot;&nbsp;&middot;&nbsp;&middot;
        <span class="pl">&nbsp;(<a target="_blank" href="http://shuo.douban.com/#!/47844141">全部</a>)</span>
    </h2>
    





<script id="shuo-widget-item" type="text/template">
    <li class="item">
        <div class="text">
            {{html shuoText}}
        </div>
    	<div class="attachment">
            <a href="{{= attachmentShortURL}}" target="_blank" title="{{= attachmentURL}}">{{= attachmentTitle}}</a>
    	</div>
        <div class="ft">
            <a href="{{= shuoURL}}" class="time-stamp" target="_blank">{{= shuoTimeStamp}}</a>
            &middot;
            <a href="{{= shuoURL}}" target="_blank">{{if commentsCount}}{{= commentsCount}}{{/if}}回复</a>
        </div>
    </li>
</script>

<div id="shuo-widget">
	<div class="body">
		<ul></ul>
        <div class="state"></div>
	</div>
</div>

<script id="tmpl-user-mentions" type="text/template">
    <a href="http://shuo.douban.com/#!/{{= uid}}" target="_blank" title="{{= screen_name}} (@{{= uid}})">{{= screen_name}}</a>
</script>
<script id="tmpl-urls" type="text/template">
    <a href="{{= url}}" title="{{= expanded_url}}" target="_blank">{{= expanded_url}}</a>
</script>
<script id="tmpl-topics" type="text/template">
    <a href="http://shuo.douban.com/#!/topics/{{= encoded_text}}" target="_blank" >{{= text}}</a>
</script>
<script type="text/javascript">
    Do.add('template',{path: 'http://img3.douban.com/js/lib/packed_jquery.tmpl2124361866.js', type: 'js'});
    Do('template',function() {
        var TMPL_TL_ITEM = $.template(null, $('#shuo-widget-item').html()),
        shuoWidget = $('#shuo-widget');

        shuoWidget.find('.state').text('加载中...');

        //error handling
        var cbSuccess = false;
        setTimeout(function() {
            if ( !cbSuccess ) {
                shuoWidget.find('.state').text('加载超时啦，刷新重试一下');       
            }
        },6000);

        $.ajax({
            dataType: 'jsonp',
            url: 'http://www.douban.com/shuo/!service/widgets/user_timeline/47844141?count=5&alt=xd&reshared=0',
            jsonp: false, 
            jsonpCallback: 'cb',
            success: renderTimeline
        });

        function renderTimeline(data) {
            cbSuccess = true;

            var id = '47844141',
                url = 'http://shuo.douban.com/#!/' + id,
                name = '【FddddddY.】',
                i = 0,
                length = data.length,
                shuoInfo = [],
                item;

            for ( i=0;i<length;i++ ) {
                item = data[i];

                if ( item.reshared_status !== undefined ) {
                // 转发的不放进去
                    continue;
                } else {
                    shuoInfo.push({
                        //common parts
                        profileURL: url,
                        profileName: name,
                        shuoText: extractText(item.text, item.entities) || '推荐',
                        shuoURL: 'http://shuo.douban.com/#!/' + id + '/status/' + item.id,
                        shuoTimeStamp: getTimestamp(item.created_at), 
                        commentsCount: item.comments_count,

                        //different parts
                        // item.attachments[0] acts as guardian value, if there's no attachments, it will be left as empty
                        attachmentShortURL: item.attachments[0] && ( item.attachments[0].href ||'http://shuo.douban.com/#!/' + id + '/status/' + item.id ),
                        attachmentURL: item.attachments[0] && item.attachments[0].expanded_href,
                        // if attachments[0].title is empty, this item must be a uploaded image
                        attachmentTitle: item.attachments[0] && ( item.attachments[0].title || '查看图片')
                    });
                }
            }
            //end of for loop
            $.tmpl(TMPL_TL_ITEM, shuoInfo).appendTo('#shuo-widget .body>ul');

            shuoWidget.find('.state').remove();

        }
        
        function getCurrentTime () {
            var d = new Date(),
                localTime = d.getTime(),
                localOffset = d.getTimezoneOffset()*60000,
                utc = localTime + localOffset,
                offset = +8,
                time = utc + (3600000*offset),
                today = new Date(time),
                year = today.getFullYear(), 
                month = today.getMonth()+1, 
                date = today.getDate(), 
                hour = today.getHours(), 
                minute = today.getMinutes(), 
                second = today.getSeconds(); 
            return year + "-" + month + "-" + date + " " + hour + ":" + minute + ":" + second;
        }
        function getTimestamp (date) {
            var now = getCurrentTime(), 
                d = date.split(" "),
                n = now.split(" "),
                dd = d[0].split("-"),
                nn = n[0].split("-"),
                diff,hs,ms,ss;

            d = dd[1] + "/" + dd[2] + "/" + dd[0] + " " + d[1]; //转化为标准时间格式(月/日/年 时:分:秒)
            n = nn[1] + "/" + nn[2] + "/" + nn[0] + " " + n[1]; //转化为标准时间格式(月/日/年 时:分:秒)
            diff = Date.parse(n)-Date.parse(d);
            ss = Math.ceil(diff / 3000);
            ms = Math.floor(diff / 60000);
            hs = Math.floor(diff / 3600000);

            if (ss === 0 ) {
                return '刚刚';
            } else if (ss < 60){
                return ss + '秒前';
            } else if (ms < 60){
                return ms + '分钟前';
            } else if (hs < 24){
                return hs + '小时前';
            } else if (dd[0] === nn[0] ) {
                d = date.split(' ')[1];
                return dd[1].replace(/^0*/, "") + '月' + dd[2].replace(/^0*/, "") + '日 ' + d.substring(0,d.lastIndexOf(':'));
            }
            return dd[0] + '年' + dd[1].replace(/^0*/, "") + '月' + dd[2].replace(/^0*/, "") + '日';
        }

        function encodeURIStr(str){
            var regexs = new Array(new RegExp('%', 'g'),
                              new RegExp(',', 'g'),
                              new RegExp('\/', 'g'),
                              new RegExp('\\?', 'g'),
                              new RegExp(':', 'g'),
                              new RegExp('@', 'g'),
                              new RegExp('&', 'g'),
                              new RegExp('=', 'g'),
                              new RegExp('\\+', 'g'),
                              new RegExp('\\$', 'g'),
                              new RegExp('#', 'g')
              );  
            var replaces = new Array('%25', '%2C','%2F','%3F','%3A','%40','%26','%3D','%2B','%24','%23');
            for (var i = 0; i < regexs.length; i++){
                str = str.replace(regexs[i], replaces[i]);
            }   
            return str;
        }

        function extractText(text,entities) {
            var urls = entities.urls,
                user_mentions = entities.user_mentions,
                topics = entities.topics,
                all_html = [], 
                all = [], 
                ret_text = '', s, e, begin, plaintext;

            $('#tmpl-user-mentions').template('TMPL_UESR_MENTIONS');
            $('#tmpl-urls').template('TMPL_URLS');
            $('#tmpl-topics').template('TMPL_TOPICS');

            all = urls.concat(user_mentions);
            all = all.concat(topics);
            all.sort(function(a,b){
                return a['indices'][0] - b['indices'][0];
            }); 

            //if ( all.length === 0 ) return emoji(text);
            if ( all.length === 0 ) return text.replace(/\ue335/g, '&#9733;'); 

            for ( var k = 0; k < all.length; k++ ) { 
                if ( all[k]['uid'] ) { 
                    all_html[k] = $.tmpl('TMPL_UESR_MENTIONS',all[k]);
                }   
                else if ( all[k]['url'] ) { 
                     all_html[k] = $.tmpl('TMPL_URLS',all[k]);
                }   
                else {
                    all[k]['encoded_text'] = encodeURIStr(all[k]['text'])
                    all_html[k] = $.tmpl('TMPL_TOPICS',all[k]);
                }   


                begin = k ? e : 0,
                s = all[k]['indices'][0];
                e = all[k]['indices'][1];
                plaintext = text.substring(begin, s).replace(/\ue335/g, '&#9733;');
                //plaintext = emoji(text.substring(begin, s));

                ret_text = ret_text + plaintext + $('<div></div>').append(all_html[k].clone()).html();

                if( k === all.length - 1) {
                    ret_text = ret_text + text.substring(e).replace(/\ue335/g, '&#9733;');
                    //ret_text = ret_text + emoji(text.substring(e));
                }   
            }   
            return ret_text;
        }
    });
</script>



    








