Do(function(){
    var more = $('.top-nav-more a.more').click(function(e){
      e.preventDefault();
      var el = $(e.currentTarget), p = el.parent(); 
      p.toggleClass('on');
      return false;
    });
    $('body').click(function(){
      more.parent().removeClass('on');
